    "version": "0.2.0",
    "configurations": [
        {
            "name": "main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [
                "--scratch",
                "--device", "cuda:0",
                "--seed", "1",
                "--dataset", "demosath",
                "--dataset_path",  "../datasets/demosath",
                "--seq_len", "24",
                "--feature", "7",
                "--missing_pattern", "block",
                "--missing_ratio", "0.2"
            ]
        }
    ]
}